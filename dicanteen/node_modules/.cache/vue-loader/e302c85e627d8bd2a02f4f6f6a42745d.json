{"remainingRequest":"C:\\Users\\ASUS\\Desktop\\kuliah\\vue_js\\diCanteen\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\ASUS\\Desktop\\kuliah\\vue_js\\diCanteen\\src\\views\\FoodDetail.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Users\\ASUS\\Desktop\\kuliah\\vue_js\\diCanteen\\src\\views\\FoodDetail.vue","mtime":1608055509377},{"path":"C:\\Users\\ASUS\\Desktop\\kuliah\\vue_js\\diCanteen\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\ASUS\\Desktop\\kuliah\\vue_js\\diCanteen\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"C:\\Users\\ASUS\\Desktop\\kuliah\\vue_js\\diCanteen\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\ASUS\\Desktop\\kuliah\\vue_js\\diCanteen\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["FoodDetail.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2DA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"FoodDetail.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"food-detail\">\r\n    <Navbar />\r\n    <div class=\"container\">\r\n      <!-- breadcrumb -->\r\n      <div class=\"row mt-4\">\r\n        <div class=\"col\">\r\n          <nav aria-label=\"breadcrumb\">\r\n            <ol class=\"breadcrumb\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/\" class=\"text-dark\">Home</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/foods\" class=\"text-dark\">Foods</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\" aria-current=\"page\">Food Order</li>\r\n            </ol>\r\n          </nav>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row mt-3\">\r\n        <div class=\"col-md-6\">\r\n          <img :src=\" '../assets/images/' + product.gambar \" class=\"img-fluid shadow\" />\r\n        </div>\r\n        <div class=\"col-md-6\">\r\n          <h2>\r\n            <strong>{{ product.nama }}</strong>\r\n          </h2>\r\n          <hr />\r\n          <h4>\r\n            Harga :\r\n            <strong>Rp. {{ product.harga }}</strong>\r\n          </h4>\r\n          <form class=\"mt-4\" v-on:submit.prevent>\r\n            <div class=\"form-group\">\r\n              <label for=\"jumlah_pemesanan\">Jumlah Pesan</label>\r\n              <input type=\"number\" class=\"form-control\" v-model=\"pesan.jumlah_pemesanan\" />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"keterangan\">Keterangan</label>\r\n              <textarea\r\n                v-model=\"pesan.keterangan\"\r\n                class=\"form-control\"\r\n                placeholder=\"Keterangan spt : Pedes, Nasi Setengah ..\"\r\n              ></textarea>\r\n            </div>\r\n\r\n            <button type=\"submit\" class=\"btn btn-success\" @click=\"pemesanan\">\r\n              <b-icon-cart></b-icon-cart>Pesan\r\n            </button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/Navbar.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"FoodDetail\",\r\n  components: {\r\n    Navbar,\r\n  },\r\n  data() {\r\n    return {\r\n      product: {},\r\n      pesan: {},\r\n    };\r\n  },\r\n  methods: {\r\n    setProduct(data) {\r\n      this.product = data;\r\n    },\r\n    pemesanan() {\r\n      if (this.pesan.jumlah_pemesanan) {\r\n        this.pesan.products = this.product;\r\n        axios\r\n          .post(\"http://localhost:3000/keranjangs\", this.pesan)\r\n          .then(() => {\r\n            this.$router.push({ path: \"/keranjang\"})\r\n            this.$toast.success(\"Sukses Masuk Keranjang\", {\r\n              type: \"success\",\r\n              position: \"top-center\",\r\n              duration: 3000,\r\n              dismissible: true,\r\n            });\r\n          })\r\n          .catch((err) => console.log(err));\r\n      } else {\r\n        this.$toast.error(\"Jumlah Pesanan Harus diisi\", {\r\n          type: \"error\",\r\n          position: \"top-right\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n        });\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:3000/products/\" + this.$route.params.id)\r\n      .then((response) => this.setProduct(response.data))\r\n      .catch((error) => console.log(error));\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}