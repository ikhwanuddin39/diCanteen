A,CAAA;AAAA,SAFA,EAGA,KAHA,CAGA,UAAA,KAAA;AAAA,iBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,SAHA;AAIA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,KAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,OAhBA;AAiBA,KAtBA;AAuBA,IAAA,QAvBA,sBAuBA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,IAAA,KAAA,KAAA,CAAA,MAAA,EAAA;AACA,aAAA,KAAA,CAAA,UAAA,GAAA,KAAA,UAAA;AACA,QAAA,KAAA,CACA,IADA,CACA,gCADA,EACA,KAAA,KADA,EAEA,IAFA,CAEA,YAAA;AAEA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,mBAAA,KAAA,CACA,MADA,CACA,sCAAA,IAAA,CAAA,EADA,EAEA,KAFA,CAEA,UAAA,KAAA;AAAA,qBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,aAFA,CAAA;AAGA,WAJA;;AAMA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,OAAA,CAAA,gBAAA,EAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,QAAA,EAAA,WAFA;AAGA,YAAA,QAAA,EAAA,IAHA;AAIA,YAAA,WAAA,EAAA;AAJA,WAAA;AAMA,SAlBA,EAmBA,KAnBA,CAmBA,UAAA,GAAA;AAAA,iBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,SAnBA;AAoBA,OAtBA,MAsBA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,iCAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA,WAFA;AAGA,UAAA,QAAA,EAAA,IAHA;AAIA,UAAA,WAAA,EAAA;AAJA,SAAA;AAMA;AACA;AAtDA,GAXA;AAmEA,EAAA,OAnEA,qBAmEA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CACA,kCADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,aAAA,MAAA,CAAA,aAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AAAA,KAFA,EAGA,KAHA,CAGA,UAAA,KAAA;AAAA,aAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,KAHA;AAIA,GAxEA;AAyEA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,aAAA,KAAA,UAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA,IAAA,EAAA;AACA,eAAA,KAAA,GAAA,IAAA,CAAA,QAAA,CAAA,KAAA,GAAA,IAAA,CAAA,gBAAA;AACA,OAFA,EAEA,CAFA,CAAA;AAGA;AALA;AAzEA,CAAA","sourcesContent":["<template>\n  <div class=\"keranjang\">\n    <Navbar :updateKeranjang=\"keranjangs\" />\n    <div class=\"container\">\n      <!-- breadcrumb -->\n      <div class=\"row mt-4\">\n        <div class=\"col\">\n          <nav aria-label=\"breadcrumb\">\n            <ol class=\"breadcrumb\">\n              <li class=\"breadcrumb-item\">\n                <router-link to=\"/\" class=\"text-dark\">Home</router-link>\n              </li>\n              <li class=\"breadcrumb-item\">\n                <router-link to=\"/foods\" class=\"text-dark\">Foods</router-link>\n              </li>\n              <li class=\"breadcrumb-item active\" aria-current=\"page\">Cart</li>\n            </ol>\n          </nav>\n        </div>\n      </div>\n\n      <div class=\"row\">\n        <div class=\"col\">\n          <h2>\n            Keranjang\n            <strong>Saya</strong>\n          </h2>\n          <div class=\"table-responsive mt-3\">\n            <table class=\"table\">\n              <thead>\n                <tr>\n                  <th scope=\"col\">#</th>\n                  <th scope=\"col\">Foto</th>\n                  <th scope=\"col\">Makanan</th>\n                  <th scope=\"col\">Keterangan</th>\n                  <th scope=\"col\">Jumlah</th>\n                  <th scope=\"col\">Harga</th>\n                  <th scope=\"col\">Total Harga</th>\n                  <th scope=\"col\">Hapus</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"(keranjang, index) in keranjangs\" :key=\"keranjang.id\">\n                  <th>{{index+1}}</th>\n                  <td>\n                    <img\n                      :src=\" '../assets/images/' + keranjang.products.gambar \"\n                      class=\"img-fluid shadow\"\n                      width=\"250\"\n                    />\n                  </td>\n                  <td>\n                    <strong>{{ keranjang.products.nama }}</strong>\n                  </td>\n                  <td>{{ keranjang.keterangan ? keranjang.keterangan : \"-\" }}</td>\n                  <td>{{ keranjang.jumlah_pemesanan }}</td>\n                  <td align=\"right\">Rp. {{ keranjang.products.harga }}</td>\n                  <td align=\"right\">\n                    <strong>Rp. {{ keranjang.products.harga*keranjang.jumlah_pemesanan }}</strong>\n                  </td>\n                  <td align=\"center\" class=\"btn btn-danger ml-3\">\n                    <b-icon-trash @click=\"hapusKeranjang(keranjang.id)\"></b-icon-trash>\n                  </td>\n                </tr>\n\n                <tr>\n                  <td colspan=\"6\" align=\"right\">\n                    <strong>Total Harga :</strong>\n                  </td>\n                  <td align=\"right\">\n                    <strong>Rp. {{ totalHarga }}</strong>\n                  </td>\n                  <td></td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n\n      <!-- Form Checkout -->\n      <div class=\"row justify-content-end\">\n        <div class=\"col-md-4\">\n          <form class=\"mt-4\" v-on:submit.prevent>\n            <div class=\"form-group\">\n              <label for=\"nama\">Nama :</label>\n              <input type=\"text\" class=\"form-control\" v-model=\"pesan.nama\" />\n            </div>\n            <div class=\"form-group\">\n              <label for=\"noMeja\">Nomor Meja :</label>\n              <input type=\"text\" class=\"form-control\" v-model=\"pesan.noMeja\" />\n            </div>\n\n            <button type=\"submit\" class=\"btn btn-danger float-right\" @click=\"checkout\">\n              <b-icon-cart></b-icon-cart>Pesan\n            </button>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Navbar from \"@/components/Navbar.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"Keranjang\",\n  components: {\n    Navbar,\n  },\n  data() {\n    return {\n      keranjangs: [],\n      pesan: {},\n    };\n  },\n  methods: {\n    setKeranjangs(data) {\n      this.keranjangs = data;\n    },\n    hapusKeranjang(id) {\n      axios\n        .delete(\"http://localhost:3000/keranjangs/\" + id)\n        .then(() => {\n          this.$toast.error(\"Sukses Hapus Keranjang\", {\n            type: \"error\",\n            position: \"top-right\",\n            duration: 3000,\n            dismissible: true,\n          });\n\n          // Update Data keranjang\n          axios\n            .get(\"http://localhost:3000/keranjangs\")\n            .then((response) => this.setKeranjangs(response.data))\n            .catch((error) => console.log(error));\n        })\n        .catch((error) => console.log(error));\n    },\n    checkout() {\n      if (this.pesan.nama && this.pesan.noMeja) {\n        this.pesan.keranjangs = this.keranjangs;\n        axios\n          .post(\"http://localhost:3000/pesanans\", this.pesan)\n          .then(() => {\n\n            // Hapus Semua Keranjang \n            this.keranjangs.map(function (item) {\n              return axios\n                .delete(\"http://localhost:3000/keranjangs/\" + item.id)\n                .catch((error) => console.log(error));\n            });\n\n            this.$router.push({ path: \"/pesanan-sukses\" });\n            this.$toast.success(\"Sukses Dipesan\", {\n              type: \"success\",\n              position: \"top-right\",\n              duration: 3000,\n              dismissible: true,\n            });\n          })\n          .catch((err) => console.log(err));\n      } else {\n        this.$toast.error(\"Nama dan Nomor Meja Harus diisi\", {\n          type: \"error\",\n          position: \"top-right\",\n          duration: 3000,\n          dismissible: true,\n        });\n      }\n    },\n  },\n  mounted() {\n    axios\n      .get(\"http://localhost:3000/keranjangs\")\n      .then((response) => this.setKeranjangs(response.data))\n      .catch((error) => console.log(error));\n  },\n  computed: {\n    totalHarga() {\n      return this.keranjangs.reduce(function (items, data) {\n        return items + data.products.harga * data.jumlah_pemesanan;\n      }, 0);\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views"}]}